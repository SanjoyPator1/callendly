{
    "source": {
      "include": ["packages/callendly/src/customCalendar/calendar.tsx"],
      "includePattern": "\\.(js|ts)x?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown",
      "./node_modules/jsdoc-ts-utils/dist/plugins/typescript"
    ],
    "opts": {
      "encoding": "utf8",
      "readme": "./README.md",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/clean-jsdoc-theme",
      "theme_opts": {
        "default_theme": "dark"
      }
    },
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "Callendly calendar"
    },
    "markdown": {
      "hardwrap": false,
      "idInHeadings": true
    }
  }
  
  